{
    "version": 2,
    "builds": [
      {
        "src": "src/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/server.js"
      }
    ],
    "env": {
      "MONGO_URI": "@mongo_uri",
      "JWT_SECRET": "@jwt_secret",
      "PORT": "5000"
    }
  }
  