{
    "version": 2,
    "builds" : [
        {
            "src": "src/server.js",
            "use": "@vercel/node"  
        }
    ],
    "routes" : [
        {
            "src": "/(.*)",
            "dest": "src/server.js",
            "methods": ["GET","PUT", "POST","PATCH", "DELETE", "OPTIONS"]
            
        }
    ]
}